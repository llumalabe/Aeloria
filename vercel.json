{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["sin1"],
  "env": {
    "NEXT_PUBLIC_API_URL": "@api_url",
    "NEXT_PUBLIC_RONIN_CHAIN_ID": "@ronin_chain_id",
    "NEXT_PUBLIC_CHARACTER_CONTRACT": "@character_contract",
    "NEXT_PUBLIC_ITEM_CONTRACT": "@item_contract",
    "NEXT_PUBLIC_TOKEN_CONTRACT": "@token_contract",
    "NEXT_PUBLIC_MARKETPLACE_CONTRACT": "@marketplace_contract"
  }
}
